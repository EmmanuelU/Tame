<?xml version="1.0" encoding="UTF-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <Preference
        android:key="cur_freq"
        style="?android:preferenceInformationStyle"
        android:title="Current Frequency"
        android:selectable="false" />

    <ListPreference
	android:title="Governor"
	android:summary=""
	android:key="governor"
    />

    <ListPreference
	android:title="Maximum Frequency"
	android:summary=""
	android:key="max_freq"
    />

    <ListPreference
	android:title="Minimum Frequency"
	android:summary=""
	android:key="min_freq"
    />

    <ListPreference
	android:title="IO Scheduler"
	android:summary=""
	android:key="iosched"
    />

    <com.emman.tame.dialogs.GPUPreference
	android:key="gpu_dialog"
	android:title="GPU Control"
	android:positiveButtonText="OK"
    />

    <ListPreference
	android:title="Multi-Core Power Saving"
	android:summary="Group more processes onto primary core."
	android:key="sched_mc"
	android:entries="@array/mc_array_entries"
	android:entryValues="@array/mc_array_values"
    />

    <CheckBoxPreference
	android:title="CPU Boost"
	android:summary="Speed up commonly used CPU tasks."
	android:key="cpu_boost"
    />

    <ListPreference
	android:title="Celox Display Undervoltage"
	android:summary="Undervolting the screen saves battery. Colors may appear different, but this feature is safe to use."
	android:key="celox_uv_panel"
	android:entries="@array/celox_uv_array_entries"
	android:entryValues="@array/celox_uv_array_values"
    />

</PreferenceScreen>
